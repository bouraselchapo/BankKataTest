package com.bank.account.service;

/**
 * Interface for all account operations.
 * 
 * @author hicham.
 *
 */
public interface IaccountService {

  /**
   * 
   */
  void createAccount();

  /**
   * @param accountNumber
   * 
   */
  void depositAmount(int accountNumber);

  /**
   * 
   */
  void withdrawAmount();

}
